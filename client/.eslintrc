{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "plugin:react/recommended",
      "airbnb"
  ],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "plugins": [
      "react"
  ],
  "rules": {
      "no-console": ["off"],
      "import/no-extraneous-dependencies": ["off"],
      "import/extensions": ["off"],
      "react/react-in-jsx-scope": ["off"],
      "react/jsx-filename-extension": ["off"]
      
  }
}